int main(){
   int it;
    scanf("%d",&it);
    while(it--){
        int x,y;
        scanf("%d %d",&x,&y);
        if(x<y) cout<<"<"<<endl;
        else if(x > y) cout<<">"<<endl;
        else
            cout<<"="<<endl;

    }
        return 0;
    }